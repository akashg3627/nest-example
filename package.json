{
  "name": "nest-sequelize-typescript",
  "version": "0.1.0",
  "description": "Nestjs sequelize typescript example",
  "author": "akashg3627",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "migration": "yarn sequelize-cli migration:generate",
    "migrate": "yarn sequelize-cli db:migrate",
    "migrate:dev": "secretsfoundry run -s \"yarn migrate\"",
    "start": "nest start",
    "start:dev": "secretsfoundry run -s \"nest start --watch\"",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "console": "ts-node -r tsconfig-paths/register src/console.ts"
  },
  "dependencies": {
    "@nestjs/axios": "^1.0.1",
    "@nestjs/common": "9.0.1",
    "@nestjs/core": "9.0.5",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "9.0.1",
    "@nestjs/platform-socket.io": "^9.2.1",
    "@nestjs/sequelize": "9.0.0",
    "@nestjs/websockets": "^9.2.1",
    "@types/helmet": "^4.0.0",
    "@types/lodash.omit": "^4.5.7",
    "axios": "^1.2.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cuid": "^2.1.8",
    "helmet": "^6.0.1",
    "moment": "^2.29.3",
    "mysql2": "2.3.3",
    "nest-winston": "^1.6.2",
    "passport": "^0.6.0",
    "pg": "^8.10.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.5",
    "secretsfoundry": "^0.1.20",
    "sequelize": "6.29.0",
    "sequelize-cli": "^6.4.1",
    "sequelize-typescript": "2.1.3",
    "typescript": "4.7.4",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.8",
    "@nestjs/config": "^2.2.0",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/testing": "^9.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.5",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.11.18",
    "@types/sequelize": "^4.28.14",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "await-outside": "^3.0.0",
    "coverage-badges": "^1.0.7",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.3.1",
    "jsonwebtoken": "^9.0.0",
    "passport-jwt": "^4.0.0",
    "prettier": "^2.6.2",
    "purdy": "^3.5.1",
    "rimraf": "^4.0.4",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "swagger-ui-express": "^4.3.0",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.5.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "rootDir": ".",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.ts"
    ],
    "globalSetup": "<rootDir>/test/db/database.seed.ts",
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "coverageReporters": [
      "text",
      "lcov",
      "json-summary"
    ]
  }
}
